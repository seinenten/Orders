@page "/states/edit/{StateId:int}"

<h3>Editar Estado</h3>

@if (state is null)
{
    <Loading/>
}
else
{
    <FormWithName @ref="stateForm" 
                    Label="Estado" 
                    Model="state" 
                    OnValidSubmit="SaveAsync" 
                    ReturnAction="Return"   />
}